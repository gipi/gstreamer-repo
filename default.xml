<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote
        name="anongit"
        fetch="git://anongit.freedesktop.org/gstreamer/"
    />
    <remote
        name="github"
        fetch="https://github.com/gipi/"
    />
    <default
        revision="master"
        remote="anongit"
    />
    <project path="build" name="gstreamer-repo" remote="github" revision="build"/>
    <project path="master/gstreamer" name="gstreamer"/>
    <project path="master/gst-plugins-base" name="gst-plugins-base"/>
    <project path="master/gst-plugins-good" name="gst-plugins-good"/>
    <project path="master/gst-plugins-bad" name="gst-plugins-bad"/>
    <project path="master/qt-gstreamer" name="qt-gstreamer"/>
</manifest>
