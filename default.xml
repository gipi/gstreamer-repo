<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote
        name="anongit"
        fetch="git://anongit.freedesktop.org/gstreamer/"
    />
    <remote
        name="github"
        fetch="https://github.com/gipi/"
    />
    <default
        revision="1.2"
        remote="anongit"
    />
    <project path="build" name="gstreamer-repo" remote="github" revision="master"/>
    <project path="1.2/gstreamer" name="gstreamer"/>
    <project path="1.2/gst-plugins-base" name="gst-plugins-base"/>
    <project path="1.2/gst-plugins-good" name="gst-plugins-good"/>
    <project path="1.2/gst-plugins-bad" name="gst-plugins-bad"/>
</manifest>
